<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mahmoud.jumperupdate">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".viewcontroler.MainActivity"
            android:label="Home"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".viewcontroler.Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".viewcontroler.WalkThrough" />
        <activity
            android:name=".viewcontroler.Search"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".viewcontroler.AdvancedSearch"
            android:label="Advanced Search"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".viewcontroler.Notification"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".viewcontroler.TripInformation"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".viewcontroler.Company"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".viewcontroler.Aboutus"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>
